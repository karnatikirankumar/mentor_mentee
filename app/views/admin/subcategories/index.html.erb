<h1>Listing subcategories</h1>

<table>
  <tr>
   
    <th>Category</th>
    <th>Name</th>
    <th></th>
    <th></th>
  </tr>

<% @subcategories.each do |subcategory| %>
  <tr>
  
    
   
    <td><%= subcategory.category.name %></td>
    <td><%= subcategory.name %></td>
    <td><%= link_to 'Show',:action=> 'show', :id => subcategory.id  %></td>
    <td><%= link_to 'Edit', edit_admin_subcategory_path(subcategory) %></td>
    <td><%= link_to 'Destroy',admin_subcategory_path(subcategory) , :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>

</table>

<br />

<%= link_to 'New Subcategory', new_admin_subcategory_path %>
