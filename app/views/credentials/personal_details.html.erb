<%= form_for(resource, :as => resource_name, :url => registration_path(resource_name)) do |f| %>
  <%= devise_error_messages! %>
<p><%= f.label :FirstName %>
  <%= f.text_field :fname %></p> 

 <p><%= f.label :LastName %>
  <%= f.text_field :lname %></p> 
 
 <p>Date of Birth: <%= f.date_select :dob, :start_year =>
1920, :use_month_numbers => true, :include_blank => true %><i>(In YYYY-MM-DD)</i></p>


 <p> Gender
 <%= radio_button("user","gender","Male") %>Male   <%= radio_button("user","gender","Female") %>Female</p>

 <p>Country
  <%= f.text_field :country %></p>

 <p>State
  <%= f.text_field :state %></p>
<p>City
  <%= f.text_field :city %></p>
<p>Phone
  <%= f.text_field :phone %></p> 
<% end %>



